<!doctype html>
<html class="no-js" lang="{{ request.locale.iso_code }}">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="">
    <link rel="canonical" href="{{ canonical_url }}">
    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>

    <!-- {%- if settings.favicon != blank -%}
      <link rel="icon" type="image/png" href="{{ settings.favicon | img_url: '32x32' }}">
    {%- endif -%} -->

    {%- unless settings.type_header_font.system? and settings.type_body_font.system? -%}
      <link rel="preconnect" href="https://fonts.shopifycdn.com" crossorigin>
    {%- endunless -%}

    <title>
      {{ page_title }}
      {%- if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif -%}
      {%- if current_page != 1 %} &ndash; Page {{ current_page }}{% endif -%}
      {%- unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless -%}
    </title>

    {% if page_description %}
      <meta name="description" content="{{ page_description | escape }}">
    {% endif %}

    {% render 'meta-tags' %}

    {{ content_for_header }}

    {%- liquid
      assign body_font_bold = settings.type_body_font | font_modify: 'weight', 'bold'
      assign body_font_italic = settings.type_body_font | font_modify: 'style', 'italic'
      assign body_font_bold_italic = body_font_bold | font_modify: 'style', 'italic'
    %}

    {% style %}
      @font-face {
        font-family: 'Gotham';
        src: url({{ 'Gotham-Book.otf' | asset_url }}) format("opentype");
        font-weight: normal;
        font-style: normal;
      }
      @font-face {
        font-family: 'Gotham-Medium';
        src: url({{ 'Gotham-Medium.otf' | asset_url }}) format("opentype");
        font-weight: normal;
        font-style: normal;
      }
      @font-face {
        font-family: 'SF Pro';
        src: url({{ 'SF-Pro-Display-Black.otf' | asset_url }}) format("opentype");
        font-weight: normal;
        font-style: normal;
      }

      html, body {
        width: 100%;
        height: 100%;
        margin: 0;
        padding: 0;
      }
      body {
        background: #FEFEFE;
        position: relative;
        overflow-x: hidden;
      }

      .container {
        width: 1366px;
        margin: 0 auto;
      }
      @media screen and (min-width: 1000px) and (max-width: 1366px) {        
        .container {
          width: calc(1366 * 100vw / 1366);
        }
      }
      @media screen and (max-width: 999px) {  
        .container {
          width: 100%;
        }
      }

      main {
        width: 100%;
        padding-top: 0;
      }      
      main.main_faq {
        padding-top: 55px;
      }
      main.main_all-products {
        padding-top: 64px;
      }
      @media screen and (min-width: 1000px) and (max-width: 1366px) { 
        main.main_faq {
          padding-top: calc(55 * 100vw / 1366);
        }
        main.main_all-products {
          padding-top: calc(64 * 100vw / 1366);
        }
      }
      @media screen and (max-width: 999px) {  
        main {
          width: 390px;
          padding-top: 80px;
          margin: 0 auto;
        }
      }
    {% endstyle %}

    {%- unless settings.type_body_font.system? -%}
      <link rel="preload" as="font" href="{{ settings.type_body_font | font_url }}" type="font/woff2" crossorigin>
    {%- endunless -%}
    {%- unless settings.type_header_font.system? -%}
      <link rel="preload" as="font" href="{{ settings.type_header_font | font_url }}" type="font/woff2" crossorigin>
    {%- endunless -%}

    <link rel="stylesheet" type="text/css" href="{{ 'ellipses.css' | asset_url }}">
    <link rel="stylesheet" type="text/css" href="{{ 'ellipses-responsive.css' | asset_url }}">

    <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
    <script>
      document.documentElement.className = document.documentElement.className.replace('no-js', 'js');
      if (Shopify.designMode) {
        document.documentElement.classList.add('shopify-design-mode');
      }
    </script>
  </head>

  <body class="gradient">
      {% if template == 'index' %}
        {% section 'header_main' %}
      {% else %}
        {% section 'header_custom' %}
      {% endif %}

    <div class="container">

      <main id="MainContent" class="content-for-layout focus-none {{ 'main_' + page.handle }}" role="main" tabindex="-1">
        {{ content_for_layout }}
      </main>

    </div>
      {% section 'footer' %}

    {% if template == 'index' %}
      <div class="ellipse-container ellipse__main-1"><div class="ellipse-wrap"><div class="ellipse"></div></div></div>
      <div class="ellipse-container ellipse__main-2"><div class="ellipse-wrap"><div class="ellipse"></div></div></div>
