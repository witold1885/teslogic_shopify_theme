    {% if page_description %}
      <meta name="description" content="{{ page_description | escape }}">
    {% endif %}

    {% render 'meta-tags' %}

    {{ content_for_header }}

    {%- liquid
      assign body_font_bold = settings.type_body_font | font_modify: 'weight', 'bold'
      assign body_font_italic = settings.type_body_font | font_modify: 'style', 'italic'
      assign body_font_bold_italic = body_font_bold | font_modify: 'style', 'italic'
    %}

    {% style %}
      @font-face {
        font-family: 'Gotham';
        src: url({{ 'Gotham-Book.otf' | asset_url }}) format("opentype");
        font-weight: normal;
        font-style: normal;
      }
      @font-face {
        font-family: 'Gotham-Medium';
        src: url({{ 'Gotham-Medium.otf' | asset_url }}) format("opentype");
        font-weight: normal;
        font-style: normal;
      }
      @font-face {
        font-family: 'SF Pro';
        src: url({{ 'SFPRODISPLAYREGULAR.OTF' | asset_url }}) format("opentype");
        font-weight: normal;
        font-style: normal;
      }
      /*@font-face {
        font-family: 'Manrope';
        src: url({{ 'fonts/Manrope-Regular.ttf' | asset_url }}) format("truetype");
        font-weight: normal;
        font-style: normal;
      }*/

      html, body {
        width: 100%;
        height: 100%;
        margin: 0;
        padding: 0;        
      }
      body {
        background: #FEFEFE;
        position: relative;
        overflow-x: hidden;
      }
      @media screen and (max-width: 575px) {
        html, body {
          overflow-x: hidden
        }
      }

      .container {
        width: 1440px;
        margin: 0 auto;
      }
      @media screen and (min-width: 576px) and (max-width: 1440px) {        
        .container {
          width: calc(1440 * 100vw / 1440);
        }
      }
      @media screen and (max-width: 575px) {  
        .container {
          width: 100%;
        }
      }

      main {
        width: 100%;
        padding-top: 0;
      }      
      /*main.main_faq, main.main_manuals, main.main_product {
        padding-top: 55px;
      }
      main.main_all-products {
        padding-top: 64px;
      }*/
      /*@media screen and (min-width: 576px) and (max-width: 1440px) { 
        main.main_faq, main.main_manuals, main.main_product {
          padding-top: calc(55 * 100vw / 1440);
        }
        main.main_all-products {
          padding-top: calc(64 * 100vw / 1440);
        }
      }*/
      @media screen and (max-width: 575px) {  
        main {
          width: calc(375 * 100vw / 375);
          padding-top: calc(16 * 100vw / 375);
          margin: 0 auto;
        }    
        main.main_faq, main.main_manuals, main.main_privacy, main.main_terms, main.main_cart {
          padding-top: calc(16 * 100vw / 375);
        }
        main.main_product {
          padding-top: calc(16 * 100vw / 375);
        }
        main.main_contact, main.main_pro, main.main_screenmate {
          padding: 0;
        }
      }
      .block_title, .title-wrapper-with-link h1 {
        width: 1206px;
        margin: 0 auto;
        font-family: 'Gotham'!important;
        font-style: normal;
        font-weight: 700!important;
        font-size: 52px;
        line-height: 64px;
        color: #000000;
        text-align: left;
        margin-left: 80px;
/*        letter-spacing: 0.02em;*/
      }
      @media screen and (min-width: 576px) and (max-width: 1440px) {
        .block_title, .title-wrapper-with-link h1 {
          width: calc(1206 * 100vw / 1440);
          font-size: calc(52 * 100vw / 1440);
          line-height: calc(64 * 100vw / 1440);
          margin-left: calc(80 * 100vw / 1440);
        }
      }
      @media screen and (max-width: 575px) {
        .block_title, .title-wrapper-with-link h1 {
          /*max-width: calc(335 * 100vw / 375);
          margin: 0 calc(20 * 100vw / 375);
          font-size: calc(36 * 100vw / 375);
          line-height: calc(36 * 100vw / 375);*/
          max-width: 335px;
          margin: 0 20px;
          font-size: 36px;
          line-height: 36px;
          font-weight: 400!important;
          font-family: 'Gotham-Medium'!important;
        }
        #main-cart-footer {
          width: calc(335 * 100vw / 375)!important;
          padding: 0!important;
        }
      }
    {% endstyle %}

    {%- unless settings.type_body_font.system? -%}
      <link rel="preload" as="font" href="{{ settings.type_body_font | font_url }}" type="font/woff2" crossorigin>
    {%- endunless -%}
    {%- unless settings.type_header_font.system? -%}
      <link rel="preload" as="font" href="{{ settings.type_header_font | font_url }}" type="font/woff2" crossorigin>
    {%- endunless -%}

    <link rel="stylesheet" type="text/css" href="{{ 'ellipses.css' | asset_url }}">
    <link rel="stylesheet" type="text/css" href="{{ 'ellipses-responsive.css' | asset_url }}">

    <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
    <script src="https://code.jquery.com/ui/1.13.2/jquery-ui.js"></script>
    <script>
      document.documentElement.className = document.documentElement.className.replace('no-js', 'js');
      if (Shopify.designMode) {
        document.documentElement.classList.add('shopify-design-mode');
      }
    </script>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-LM54HXMV7K"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'G-LM54HXMV7K');
    </script>

{% render 'analyzify-product-datalayer.liquid' %}    
    
  </head>

  <body class="gradient">
    <div {% if template == 'index' %}style="overflow: hidden;"{% endif %}>
      {% if template == 'index' %}
        {% section 'header_main' %}
      {% elsif page.handle == 'pro' %}
        {% section 'header_pro' %}
      {% elsif page.handle == 'screenmate' or page.handle == 'screenmate-done' %}
        {% section 'header_screenmate' %}
      {% elsif page.handle == 'app-redirect' %}
        <div></div>
      {% else %}
        {% section 'header_custom' %}
      {% endif %}

    <!-- <div class="container"> -->
      {% if page.handle != blank %}
        {% assign handle = page.handle %}
      {% else %}
        {% assign handle = template %}
      {% endif %}
      
      <main id="MainContent" class="content-for-layout focus-none main_{{ handle }}" role="main" tabindex="-1">
        {{ content_for_layout }}
      </main>

    <!-- </div> -->
    {% if page.handle != 'screenmate' %}
      {% section 'footer' %}
    {% endif %}
    </div>

    <script>
      window.shopUrl = '{{ shop.url }}';
      window.routes = {
        cart_add_url: '{{ routes.cart_add_url }}',
        cart_change_url: '{{ routes.cart_change_url }}',
        cart_update_url: '{{ routes.cart_update_url }}',
        predictive_search_url: '{{ routes.predictive_search_url }}'
      };

      window.cartStrings = {
        error: `{{ 'sections.cart.cart_error' | t }}`,
        quantityError: `{{ 'sections.cart.cart_quantity_error_html' | t: quantity: '[quantity]' }}`
      }

      window.variantStrings = {
        addToCart: `{{ 'products.product.add_to_cart' | t }}`,
        soldOut: `{{ 'products.product.sold_out' | t }}`,
        unavailable: `{{ 'products.product.unavailable' | t }}`,
      }

      window.accessibilityStrings = {
        imageAvailable: `{{ 'products.product.media.image_available' | t: index: '[index]' }}`,
        shareSuccess: `{{ 'general.share.success_message' | t }}`,
        pauseSlideshow: `{{ 'sections.slideshow.pause_slideshow' | t }}`,
        playSlideshow: `{{ 'sections.slideshow.play_slideshow' | t }}`,
      }
    </script>
  </body>
</html>
