      <div class="ellipse-container-mobile ellipse__main-mobile-5"><div class="ellipse-wrap"><div class="ellipse"></div></div></div>
      <div class="ellipse-container-mobile ellipse__main-mobile-6"><div class="ellipse-wrap"><div class="ellipse"></div></div></div>
      <div class="ellipse-container-mobile ellipse__main-mobile-7"><div class="ellipse-wrap"><div class="ellipse"></div></div></div>
      <div class="ellipse-container-mobile ellipse__main-mobile-8"><div class="ellipse-wrap"><div class="ellipse"></div></div></div>
      <div class="ellipse-container-mobile ellipse__main-mobile-9"><div class="ellipse-wrap"><div class="ellipse"></div></div></div>
      <div class="ellipse-container-mobile ellipse__main-mobile-10"><div class="ellipse-wrap"><div class="ellipse"></div></div></div>
      <div class="ellipse-container-mobile ellipse__main-mobile-11"><div class="ellipse-wrap"><div class="ellipse"></div></div></div>
      <div class="ellipse-bottom-container">
        <div class="ellipse-bottom-wrap">
          <div class="ellipse-bottom"></div>
        </div>
      </div>
    {% elsif page.handle == 'faq' %}
      <div class="ellipse-container ellipse__faq-1"><div class="ellipse-wrap"><div class="ellipse"></div></div></div>
      <div class="ellipse-container ellipse__faq-2"><div class="ellipse-wrap"><div class="ellipse"></div></div></div>
      <div class="ellipse-container ellipse__faq-3"><div class="ellipse-wrap"><div class="ellipse"></div></div></div>
      <div class="ellipse-container ellipse__faq-4"><div class="ellipse-wrap"><div class="ellipse"></div></div></div>
