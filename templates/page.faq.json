        "607d0c5c-e1a8-4873-be59-e4c596186b15": {
          "type": "question",
          "settings": {
            "chapter_id": "5",
            "question": "Does Teslogic fit all Model 3\/Y or are there any exceptions?",
            "answer": "Teslogic is compatible with all Model 3\/Y with no exceptions. Please make sure to correctly list your Tesla model in the order form as the wiring differs depending on the model and year of the vehicle."
          }
        },
        "df090883-f0b7-447e-9ae4-f993a2b25557": {
          "type": "question",
          "settings": {
            "chapter_id": "5",
            "question": "What does US Spec and non-US Spec mean?",
            "answer": "When selecting Model 3 2019-2020, please be aware that the kits differ for US Spec and non-US Spec. The kit classification is based on the country where your Tesla is registered (not where it was manufactured). US Spec covers the USA, Canada, and Mexico, while non-US Spec includes all other countries."
          }
        },
        "366cec18-ea9e-4fa6-bcd5-229bd8236ffc": {
          "type": "question",
          "settings": {
            "chapter_id": "5",
            "question": "Is there a version of Teslogic for Model S or X?",
            "answer": "Currently, there are no such versions available. Although we don't have an exact timeline, we plan to develop them in the future."
          }
        },
        "a0405dcf-1763-44c2-aee2-79e8c182d253": {
          "type": "question",
          "settings": {
            "chapter_id": "5",
            "question": "What versions of iOS\/Android is the transmitter compatible with?",
            "answer": "Teslogic transmitter is compatible with all Apple mobile devices with iOS 12.2 or higher and all Android devices with version 5.0 or higher. Using older devices, may cause a noticeable decrease in the data exchange rate."
          }
        },
        "f2084917-d299-4cda-9192-dfd8a74f10a2": {
          "type": "question",
          "settings": {
