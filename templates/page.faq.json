        "51f2a674-dfc5-431d-913e-d745a79dd5cd": {
          "type": "question",
          "settings": {
            "chapter_id": "2",
            "question": "Do I need to remove Teslogic before visiting service center?",
            "answer": "As a precautionary measure, we recommend removing Teslogic from your car before visiting a Tesla service center. This is to prevent any potential issues with the service staff."
          }
        },
        "e02e68a5-fa29-4f4c-8fcc-ba7f28877884": {
          "type": "question",
          "settings": {
            "chapter_id": "2",
            "question": "Can using a transmitter harm my car?",
