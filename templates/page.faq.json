        "c3401778-1354-4503-b197-f8b5d9a622a9": {
          "type": "question",
          "settings": {
            "chapter_id": "2",
            "question": "Is there a manufacturer's warranty for the Teslogic transmitter?",
            "answer": "The Teslogic transmitter comes with a comprehensive 3-year warranty. If you encounter any issues or if the transmitter malfunctions, please get in touch with us, and we'll provide assistance."
          }
        },
        "97e4f599-0a93-4af5-a281-4a14ec235abc": {
